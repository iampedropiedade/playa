<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="Leafletdrop Dashboard">
        <meta name="generator" content="Symfony">
        <link rel="apple-touch-icon" sizes="180x180" href="{{ asset('build/app/images/apple-touch-icon.png') }}">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ asset('build/app/images/favicon-32x32.png') }}">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ asset('build/app/images/favicon-16x16.png') }}">
        <title>{% block title %}Playa{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app', null, 'appConfig') }}
        {% endblock %}
    </head>
    <body class="text-white bg-dark">
        <main id="app">
            <div class="loading" v-if="false">
                <i class="fas fa-circle-notch fa-spin fa-10x text-light"></i>
            </div>
            <div v-cloak>
                {% block body %}{% endblock %}
            </div>
        </main>
        {% block javascripts %}
            {{ encore_entry_script_tags('app', null, 'appConfig') }}
        {% endblock %}
    </body>
</html>
